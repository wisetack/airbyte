{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "date": {
      "type": "string"
    },
    "date_epoch": {
      "type": "integer"
    },
    "astro": {
      "type": "object",
      "properties": {
        "sunrise": {
          "type": "string"
        },
        "sunset": {
          "type": "string"
        },
        "moonrise": {
          "type": "string"
        },
        "moonset": {
          "type": "string"
        },
        "moon_phase": {
          "type": "string"
        },
        "moon_illumination": {
          "type": "integer"
        }
      }
    },
    "mintemp": {
      "type": "integer"
    },
    "maxtemp": {
      "type": "integer"
    },
    "avgtemp": {
      "type": "integer"
    },
    "totalsnow": {
      "type": "integer"
    },
    "sunhour": {
      "type": "number"
    },
    "uv_index": {
      "type": "integer"
    },
    "location": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "region": {
          "type": "string"
        },
        "lat": {
          "type": "string"
        },
        "lon": {
          "type": "string"
        },
        "timezone_id": {
          "type": "string"
        },
        "localtime": {
          "type": "string"
        },
        "localtime_epoch": {
          "type": "integer"
        },
        "utc_offset": {
          "type": "string"
        }
      }
    }
  }
}